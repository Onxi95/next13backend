{
  "kind": "collectionType",
  "collectionName": "orders",
  "info": {
    "singularName": "order",
    "pluralName": "orders",
    "displayName": "orders",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "total": {
      "type": "integer",
      "required": true,
      "min": 0
    },
    "status": {
      "type": "enumeration",
      "enum": [
        "done",
        "payment_failed",
        "payment_processed",
        "payment_processing",
        "ordered"
      ],
      "required": true,
      "default": "ordered"
    },
    "order_items": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::order-item.order-item"
    }
  }
}
